#!/usr/bin/env zsh

# Easier navigation: .., ..., ...., ..... and ~
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"                    # `cd` is probably faster to type though

# Shortcuts
alias dc="cd ~/Documents"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias de="cd ~/Developer"
